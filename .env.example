# This file contains environment variables for the MAAP Agent Builder
# Copy this file to .env and modify as needed

# LLM API Keys
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
FIREWORKS_API_KEY=
COHERE_API_KEY=
TOGETHER_API_KEY=
VOYAGEAI_API_KEY=

# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=agent_builder

# Application Settings
LOG_LEVEL=INFO
FLASK_SECRET_KEY=change_me_in_production
AGENT_CONFIG_PATH=config/agents.yaml

# Checkpoint Configuration
CHECKPOINT_ENABLED=false
CHECKPOINT_PROVIDER=mongodb
CHECKPOINT_URI=${MONGODB_URI}
CHECKPOINT_DB=${MONGODB_DATABASE}
